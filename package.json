{
  "name": "suspicious-serverless",
  "version": "1.0.0",
  "description": "Serverless based suspicious content analysis service",
  "main": "index.js",
  "scripts": {
    "test": "mocha -r ts-node/register -t 20000 --exit src/**/__test__/**/*.ts",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "deploy": "env PUPPETEER_SKIP_CHROMIUM_DOWNLOAD=1 serverless deploy --verbose --aws-s3-accelerate",
    "deploy:stage": "npm run deploy -- -s stage",
    "deploy:prod": "npm run deploy -- -s prod",
    "info:stage": "sls info -s stage",
    "info:prod": "sls info -s prod"
  },
  "engines": {
    "node": "6.10.*"
  },
  "author": "MooYeol Lee <mooyoul@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@types/bluebird": "3.5.26",
    "@types/chai": "4.1.7",
    "@types/debug": "4.1.4",
    "@types/linkify-it": "2.1.0",
    "@types/lodash": "4.14.124",
    "@types/mocha": "5.2.6",
    "@types/nock": "10.0.2",
    "@types/node": "8.10.48",
    "@types/puppeteer": "1.6.4",
    "@types/request": "2.48.1",
    "@types/sinon": "7.0.11",
    "@vingle/serverless-tag-plugin": "1.1.2",
    "chai": "4.2.0",
    "mocha": "6.1.4",
    "nock": "10.0.6",
    "serverless": "1.42.0",
    "serverless-prune-plugin": "1.3.2",
    "serverless-webpack": "5.3.0",
    "sinon": "7.3.2",
    "ts-loader": "6.0.0",
    "ts-node": "8.1.0",
    "tslint": "5.16.0",
    "typescript": "3.4.5",
    "webpack": "4.31.0"
  },
  "dependencies": {
    "@serverless-chrome/lambda": "^1.0.0-55",
    "bluebird": "^3.5.4",
    "debug": "^4.1.1",
    "linkify-it": "^2.1.0",
    "lodash": "^4.17.11",
    "puppeteer": "~1.6.2",
    "request": "^2.88.0",
    "vingle-corgi": "^2.1.0"
  }
}
